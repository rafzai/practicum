import numpy as np

# Coefficient matrix
A = np.array([
    [-6, 5, 8, 8],
    [5, 2, 4, 5],
    [7, 1, 2, 5],
    [-6, 8, 8, 5]
])

# Constant terms
B = np.array([101, 1, -63, -1])

# Solving the system using numpy's linear algebra solver
X = np.linalg.solve(A, B)

print("Solution using matrix method:")
print(X)
