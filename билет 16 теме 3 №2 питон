import numpy as np
from scipy.integrate import quad

# Define the function to integrate
def g(x):
    return np.exp(-2 * x) * np.sin(16 * x)

# Use scipy's quad function for numerical integration
result, error = quad(g, 0, np.inf)

print(f"Result of the integral: {result:.6f}")
print(f"Estimated error: {error:.6f}")
